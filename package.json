{
  "name": "pmt",
  "author": {
    "name": "John Galt",
    "email": "kgorodetsky@yandex.ru"
  },
  "version": "0.0.0",
  "main": "target/app.js",
  "build": {
    "appId": "com.example.myapp",
    "files": [
      "./target/**/*"
    ],
    "linux": {
      "target": "deb"
    },
    "asar": false
  },
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run main:dev\" \"npm run renderer:dev\"",
    "clean": "rimraf ./dist",
    "renderer:dist": "ng build --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "main:dev": "cross-env NODE_ENV=development webpack --config ./config/webpack.main.config.js -w",
    "main:dist": "cross-env NODE_ENV=production webpack --config ./config/webpack.main.config.js",
    "renderer:dev": "ng serve",
    "electron:dev": "electron .",
    "prepackage": "npm run clean && npm run renderer:dist && npm run main:dist",
    "package": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "lodash-es": "4.17.15",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.4.1",
    "@angular-devkit/build-angular": "0.803.20",
    "@angular/cli": "8.3.20",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/dom-inputevent": "1.0.5",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "4.17.3",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "5.2.0",
    "concurrently": "5.0.0",
    "cross-env": "5.2.0",
    "electron": "9.4.0",
    "electron-builder": "21.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "4.4.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-cli": "3.3.5"
  }
}
